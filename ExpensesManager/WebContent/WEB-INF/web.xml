<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ExpensesManager</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>AllAction</servlet-name>
    <servlet-class>com.expence.action.AllAction</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/AllAction</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/addexpenses.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/register.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/viewexpences.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/search.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/addtransaction.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/viewtransaction.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/feedback.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/viewfeedback.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllAction</servlet-name>
    <url-pattern>/piechart</url-pattern>
  </servlet-mapping>
  
</web-app>